{
  "name": "temply",
  "version": "1.0.0",
  "description": "Template bot for Telegram",
  "main": "dist/app.js",
  "repository": "https://github.com/backmeupplz/temply",
  "author": "backmeupplz <backmeupplz@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "distribute": "yarn build-ts && node dist/index.js",
    "develop": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"yarn watch-ts\" \"yarn watch-js\"",
    "build-ts": "tsc --skipLibCheck",
    "watch-ts": "tsc -w --skipLibCheck",
    "watch-js": "nodemon dist/app.js"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/js-yaml": "^3.11.4",
    "@types/node": "^10.12.18",
    "dotenv": "^6.2.0",
    "js-yaml": "^3.12.1",
    "telegraf": "^3.26.0",
    "telegraf-i18n": "^6.4.0",
    "typegoose": "^5.4.1",
    "typescript": "^3.2.2"
  }
}
